    "@irys/upload": "^0.0.3",
    "@irys/upload-ethereum": "^0.0.3",
    "@lens-protocol/metadata": "^1.2.0",
    "@prisma/client": "^5.19.1",
    "base64url": "^3.0.1",
    "hono": "^4.5.8",
    "linkedom": "^0.18.4",
    "ua-parser-js": "2.0.0-beta.3",
    "viem": "^2.21.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tape.xyz/abis": "workspace:*",
    "@tape.xyz/config": "workspace:*",
    "@tape.xyz/constants": "workspace:*",
    "@tape.xyz/generic": "workspace:*",
    "@tape.xyz/server": "workspace:*",
